{
  "name": "pomi-data-etl",
  "version": "0.1.0",
  "description": "Patient Online Management Information processing",
  "main": "runner.js",
  "scripts": {
    "coverage-check": "istanbul check-coverage --config .istanbul.yml",
    "coverage-generate": "yarn run istanbul -- cover _mocha -- --recursive test",
    "coverage-upload": "cat ./coverage/lcov.info | coveralls",
    "git-hook": "yarn run lint && yarn run coverage-generate && yarn run coverage-check",
    "istanbul": "NODE_ENV=test istanbul",
    "lint": "eslint --ext .js,.json .",
    "lint-watch": "esw --watch .",
    "precommit": "yarn run git-hook",
    "prepush": "yarn run snyk -- test",
    "start": "node schedule.js",
    "test": "snyk test && mocha --recursive",
    "test-watch": "yarn run test -- --watch --reporter min",
    "upload-coverage-coveralls": "cat ./coverage/lcov.info | coveralls",
    "postrewrite": "yarn run git-hook",
    "snyk": "snyk",
    "snyk-travis": "yarn run snyk -- test --org=nhsuk && yarn run snyk -- monitor --org=nhsuk",
    "travis": "yarn run git-hook && yarn run coverage-upload && yarn run snyk-travis"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhsuk/pomi-data-etl.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhsuk/pomi-data-etl/issues"
  },
  "homepage": "https://github.com/nhsuk/pomi-data-etl#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "csv-parse": "^1.2.0",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-watch": "^2.1.14",
    "husky": "^0.13.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "snyk": "^1.30.1"
  },
  "dependencies": {
    "bunyan": "^1.8.8",
    "csv": "^1.1.1",
    "node-schedule": "^1.2.1"
  }
}

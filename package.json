{
  "name": "pomi-data-etl",
  "version": "0.1.0",
  "description": "Patient Online Management Information processing",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "NODE_ENV=test mocha --recursive test/unit",
    "lint": "eslint --ext .js,.json .",
    "watch-test": "npm run test -- --watch --reporter min",
    "istanbul": "NODE_ENV=test istanbul",
    "generate-coverage": "npm run istanbul -- cover _mocha -- --recursive test/unit",
    "check-coverage": "istanbul check-coverage --config .istanbul.yml",
    "travis": "npm run lint && npm run generate-coverage && npm run check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhsuk/pomi-data-etl.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhsuk/pomi-data-etl/issues"
  },
  "homepage": "https://github.com/nhsuk/pomi-data-etl#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-watch": "^2.1.14",
    "istanbul": "^0.4.5",
    "nodemon": "^1.11.0"
  }
}
